<h3 class>Dashboard for: <%= current_gym.name%></h3>

<hr>
<%= current_gym.uid %>
<hr>
<div class="card-group">
   <div class="card text-center">
   <div class="card-body">
      <%= current_gym.members.count %> <br> Members
      </div>
   </div>
   <div class="card text-center">
   <div class="card-body">
      <%= current_gym.gym_classes.count %> <br> Classes
      </div>
   </div>
   <div class="card text-center">
   <div class="card-body">
   <% binding.pry %>
      <%= current_gym.scheduled_classes.todays_classes.count%> <br> Classes Today
      </div>
   </div>
</div>
<br>
<div class="card-group">
   <div class="card bg-primary">
      <%= link_to 'Create Gym Member', new_member_path, class: "card-body text-center"%>
   </div>

   <div class="card bg-success">
      <%= link_to 'Creat Class', new_gym_class_path, class: "card-body text-center"%>
   </div>
</div>
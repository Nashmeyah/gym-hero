<%= form_for @member do |f| %>
   <%=f.label "Name" %>
   <%=f.text_field :name %>
   <br>
   <%=f.label "Email:" %>
   <%=f.email_field :email %>
   <br>
   <%=f.label "Age" %>
   <%=f.number_field :age %>
   <br>
   <%=f.label "Phone Number:" %>
   <%=f.telephone_field :phone_number %>
   <br>
   <%=f.submit%>
<% end %>
<%= form_for @member do |f| %>
   <%=f.label "First Name:" %>
   <%=f.text_field :first_name %>
   <br>
   <%=f.label "Last Name:" %>
   <%=f.text_field :last_name %>
   <br>
   <%=f.label "Email:" %>
   <%=f.email_field :email %>
   <br>
   <%=f.label "Phone Number:" %>
   <%=f.telephone_field :phone_number %>
   <br>
   <%=f.submit%>
<% end %>